#include<iostream>
using namespace std;
struct student
{
	int rollno;
	char name[50];
	float marks;
	
}s[5];
int main()
{
	cout<<"     STUDENT INFORMATION     "<<endl;
	for(int i=0;i<5;i++)
	{
		s[i].rollno=i+1;
		cout<<"RollNo: "<<s[i].rollno<<endl;
		cout<<"enter the name"<<endl;
		cin>>s[i].name;
		cout<<"enter the marks"<<endl;
		cin>>s[i].marks;
		
		
	}
}
